<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout android:layout_width="fill_parent" android:layout_height="fill_parent"
    xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <ScrollView android:layout_width="fill_parent" android:layout_height="fill_parent">
        <LinearLayout android:orientation="vertical" android:layout_width="fill_parent" android:layout_height="wrap_content">
            <EditText android:id="@+id/StoreSelection" android:background="@null" android:layout_width="fill_parent" android:layout_height="wrap_content" android:ems="10" android:inputType="textPersonName" />
            <TextView android:textSize="18.0sp" android:textColor="@android:color/background_dark" android:id="@+id/textView2" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="Please select coax cable:" app:fontFamily="sans-serif" />

            <Spinner
                android:id="@+id/CoaxSpinner"
                android:layout_width="362dp"
                android:layout_height="wrap_content" />

            <TextView android:textSize="18.0sp" android:textColor="@android:color/background_dark" android:id="@+id/textView3" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="Select frequency:" app:fontFamily="sans-serif" />
            <SeekBar android:id="@+id/seekBar2" android:background="@null" android:layout_width="fill_parent" android:layout_height="wrap_content" style="@style/Widget.AppCompat.SeekBar" />

            <TextView android:textSize="18.0sp" android:textColor="@android:color/background_dark" android:id="@+id/textView4" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="Frequency (MHz):" app:fontFamily="sans-serif" />
            <EditText android:id="@+id/FrequencyShow" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="0" android:ems="10" android:inputType="numberDecimal" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/textView6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Enter cable length :"
                    android:textColor="@android:color/background_dark"
                    android:textSize="18.0sp"
                    app:fontFamily="sans-serif" />

                <Spinner
                    android:id="@+id/spinner2"
                    android:layout_width="103dp"
                    android:layout_height="match_parent"
                    android:layout_toRightOf="@id/textView6" />

            </RelativeLayout>
            <EditText android:id="@+id/CableLength" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="0" android:ems="10" android:inputType="textPersonName|numberDecimal|phone|textCapWords" />

            <TextView android:textSize="18.0sp" android:textColor="@android:color/background_dark" android:id="@+id/textView5" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="Attenuation at chosen frequency (dB/m):" app:fontFamily="sans-serif" />
            <EditText android:id="@+id/AttenuationatFrequency" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="0" android:ems="10" android:inputType="textPersonName|numberDecimal|phone|textCapWords" />
            <TextView android:textSize="18.0sp" android:textColor="@android:color/background_dark" android:id="@+id/textView7" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="Total Loss (dB):" app:fontFamily="sans-serif" />
            <EditText android:id="@+id/TotalLoss" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="0" android:ems="10" android:inputType="textPersonName|numberDecimal|phone|textCapWords" />
            <TextView android:textSize="18.0sp" android:textStyle="bold" android:textColor="@android:color/background_dark" android:id="@+id/textView20" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="Cable Details:" android:textAllCaps="true" app:fontFamily="sans-serif" />

            <TextView
                android:id="@+id/title_manufacturer"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:text="Manufacturer:"
                android:textColor="@android:color/background_dark"
                android:textSize="18.0sp" />

            <EditText
                android:id="@+id/Manufacturer"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="@null"
                android:ems="10"
                android:inputType="textPersonName" />

            <TextView
                android:id="@+id/Diameter"
                android:textStyle="bold"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Diameter(mm):"
                android:textColor="@android:color/background_dark"
                android:textSize="18.0sp" />

            <TextView
                android:id="@+id/title_inner"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Inner conductor:"
                android:layout_marginLeft="20dp"
                android:textColor="@android:color/background_dark"
                android:textSize="18.0sp" />

            <EditText android:id="@+id/InnerConductor" android:layout_marginLeft="20dp" android:background="@null" android:layout_width="fill_parent" android:layout_height="wrap_content" android:ems="10" android:inputType="textPersonName" />
            <TextView android:textSize="18.0sp" android:layout_marginLeft="20dp" android:textColor="@android:color/background_dark" android:id="@+id/title_dielectric" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="Dielectric:" />
            <EditText android:id="@+id/Dielectric" android:layout_marginLeft="20dp" android:background="@null" android:layout_width="fill_parent" android:layout_height="wrap_content" android:ems="10" android:inputType="textPersonName" />
            <TextView android:textSize="18.0sp" android:layout_marginLeft="20dp" android:textColor="@android:color/background_dark" android:id="@+id/title_shield1" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="Shield (outer conductor #1):" />
            <EditText android:id="@+id/Shield1" android:layout_marginLeft="20dp" android:background="@null" android:layout_width="fill_parent" android:layout_height="wrap_content" android:ems="10" android:inputType="textPersonName" />
            <TextView android:textSize="18.0sp" android:layout_marginLeft="20dp" android:textColor="@android:color/background_dark" android:id="@+id/title_shield2" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="Shield (outer conductor #2):" />
            <EditText android:id="@+id/Shield2" android:layout_marginLeft="20dp" android:background="@null" android:layout_width="fill_parent" android:layout_height="wrap_content" android:ems="10" android:inputType="textPersonName" />
            <TextView android:textSize="18.0sp" android:layout_marginLeft="20dp" android:textColor="@android:color/background_dark" android:id="@+id/title_jacket" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="Jacket:" />
            <EditText android:id="@+id/Jacket" android:layout_marginLeft="20dp" android:background="@null" android:layout_width="fill_parent" android:layout_height="wrap_content" android:ems="10" android:inputType="textPersonName" />

            <TextView
                android:id="@+id/title_jacketTolerance2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Jacket Tolerance:"
                android:layout_marginLeft="20dp"
                android:textColor="@android:color/background_dark"
                android:textSize="18.0sp" />

            <EditText
                android:id="@+id/JacketTolerance"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="@null"
                android:layout_marginLeft="20dp"
                android:ems="10"
                android:inputType="textPersonName" />

            <TextView
                android:id="@+id/BendRadius"
                android:textStyle="bold"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:text="BendRadius(mm):"
                android:textColor="@android:color/background_dark"
                android:textSize="18.0sp" />

            <TextView android:textSize="18.0sp" android:layout_marginLeft="20dp" android:textColor="@android:color/background_dark" android:id="@+id/title_oneTime" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="One time bend radius:" />
            <EditText android:id="@+id/OneTime" android:layout_marginLeft="20dp" android:background="@null" android:layout_width="fill_parent" android:layout_height="wrap_content" android:ems="10" android:inputType="textPersonName" />
            <TextView android:textSize="18.0sp" android:layout_marginLeft="20dp" android:textColor="@android:color/background_dark" android:id="@+id/title_repeated" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="Repeated bend radius:" />
            <EditText android:id="@+id/Repeated" android:layout_marginLeft="20dp" android:background="@null" android:layout_width="fill_parent" android:layout_height="wrap_content" android:ems="10" android:inputType="textPersonName" />
            <TextView android:textSize="18.0sp" android:layout_marginLeft="20dp" android:textColor="@android:color/background_dark" android:id="@+id/title_installation" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="Installation:" />
            <EditText android:id="@+id/Installation" android:layout_marginLeft="20dp" android:background="@null" android:layout_width="fill_parent" android:layout_height="wrap_content" android:ems="10" android:inputType="textPersonName" />
            <TextView android:textSize="18.0sp" android:layout_marginLeft="20dp" android:textColor="@android:color/background_dark" android:id="@+id/title_static" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="Static bend radius:" />
            <EditText android:id="@+id/Static" android:layout_marginLeft="20dp" android:background="@null" android:layout_width="fill_parent" android:layout_height="wrap_content" android:ems="10" android:inputType="textPersonName" />

            <TextView
                android:id="@+id/title_dynamic2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Dynamic bend radius:"
                android:textColor="@android:color/background_dark"
                android:layout_marginLeft="20dp"
                android:textSize="18.0sp" />

            <EditText
                android:id="@+id/Dynamic"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="@null"
                android:layout_marginLeft="20dp"
                android:ems="10"
                android:inputType="textPersonName" />

            <TextView
                android:id="@+id/maxBend"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:text="Maximum bend radius:"
                android:textColor="@android:color/background_dark"
                android:textSize="18.0sp" />

            <EditText
                android:id="@+id/dynamic"
                android:layout_marginLeft="20dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@null"
                android:ems="10"
                android:inputType="textPersonName" />
        </LinearLayout>
    </ScrollView>
</androidx.drawerlayout.widget.DrawerLayout>